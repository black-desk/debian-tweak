#!/bin/sh

if dmidecode | grep "ThinkPad L14 Gen 2" -q; then
        IO_GITHUB_BLACKDESK_DEBIANTWEAK_IS_THINKPAD_L14_GEN2=1
fi

# https://unix.stackexchange.com/a/166751
if cat /sys/class/power_supply/*/type | grep Battery -q; then
        IO_GITHUB_BLACKDESK_DEBIANTWEAK_HAS_BATTERY=1
fi

echo "IO_GITHUB_BLACKDESK_DEBIANTWEAK_IS_THINKPAD_L14_GEN2=$IO_GITHUB_BLACKDESK_DEBIANTWEAK_IS_THINKPAD_L14_GEN2"
echo "IO_GITHUB_BLACKDESK_DEBIANTWEAK_HAS_BATTERY=$IO_GITHUB_BLACKDESK_DEBIANTWEAK_HAS_BATTERY"
